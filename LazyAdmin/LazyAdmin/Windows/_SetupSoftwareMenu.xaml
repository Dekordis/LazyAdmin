        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LazyAdmin.Windows"
        mc:Ignorable="d"
        Title="_SetupSoftwareMenu" Height="450" Width="800" WindowStyle="None" Background="Transparent" ResizeMode="NoResize">
    <Viewbox Stretch="Fill">
        <Grid Height="434.04" Width="800">
            <Grid x:Name="_HeaderButtonGrid" HorizontalAlignment="Center" VerticalAlignment="Center" Height="434" RenderTransformOrigin="0.5,0.5" Width="800">
                <Button x:Name="_SingleRun" Content="Single run of script" HorizontalAlignment="Left" Margin="72,39,0,0" VerticalAlignment="Top" Width="205" Height="73" Click="_TestButton_Click"/>
                <Button Content="Button" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top" Height="73" Width="164" Click="Button_Click"/>
            </Grid>

        </Grid>
    </Viewbox>
</Window>